<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>const ADDED_CLASS = "added";
function handleAdd() {
    const hasClass = title.classList.contains(ADDED_CLASS );
    if(hasClass) {
    title.classList.add(ADDED_CLASS);
    }
}
